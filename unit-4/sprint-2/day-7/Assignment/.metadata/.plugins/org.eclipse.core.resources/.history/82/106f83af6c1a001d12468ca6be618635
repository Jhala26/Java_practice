package com.masai4;

public class Main {

	public static void main(int[] value) {
		
		int n = value.length;
		int sum = 1;
		
		if(n == 1) {
			for(int i=1;i<=value[0];i++) {
				sum *= i;
			}
			System.out.println(sum);
		}
		else if(n == 2) {
			int val = Math.abs(value[0] - value[1]);
			for(int i=1;i<=val;i++) {
				sum *= i;
			}
			System.out.println(sum);
		}
		else {
			System.out.println("Error");
		}

	}

}
